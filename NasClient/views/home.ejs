  <script>
    // ä½¿ç”¨ Vue3 + Element Plus ç®¡ç†äº¤äº’ï¼ˆå¤åˆ¶æç¤ºç­‰ï¼‰
    (() => {
      const { createApp, ref } = Vue;
      const { ElMessage } = ElementPlus;
      createApp({
        setup() {
          const percent = ref(65);
          const used = ref(6500);
          const total = ref(10000);
          const savingAuto = ref(false);
          const srvAuto = ref(false);
          // å·²ç§»é™¤ï¼šinviteCode / copyCode
          // Element Plus è¿›åº¦æ¡é¢œè‰²ä¸æ–‡æœ¬æ ¼å¼ï¼ŒæŒ‰éœ€æ±‚å°†ç¯çŠ¶è¿›åº¦æ¡æ”¹ä¸ºç™½è‰²
          const whiteColor = ref('#ffffff');
          const formatPercent = (p) => `${p}%`;
          const goLogin = () => { window.location.href = '/login'; };
          const goFiles = () => { window.location.href = '/blank'; };
          // æ¥å…¥ i18nï¼šç¡®ä¿è¯­è¨€åˆ‡æ¢å data-i18n èŠ‚ç‚¹ä¼šè¢« apply()ï¼›æœ¬é¡µæ— éœ€åœ¨æ¨¡æ¿ä¸­è°ƒç”¨ t()
          const lang = ref(window.I18N ? window.I18N.getLang() : 'zh-CN');
          if (window.I18N && window.I18N.onChange) { window.I18N.onChange(l => { lang.value = l; }); }
          const t = (k, params) => { lang.value; return (window.I18N && window.I18N.t) ? window.I18N.t(k, params) : k; };

          // æ‹‰å–åç«¯è®¾ç½®ï¼Œåæ˜ å½“å‰æ˜¯å¦å·²å¼€å¯è‡ªåŠ¨ä»»åŠ¡
          const refreshSrvAuto = async () => {
            try {
              const r = await fetch('/api/settings');
              const d = r && r.ok ? await r.json() : null;
              if (d && d.ok && d.data) {
                srvAuto.value = !!d.data.autoGenerate;
              }
            } catch (_) { }
          };

          // é¦–é¡µæŒ‰é’®ï¼šä¸æ–‡ä»¶é¡µåŒåŠŸèƒ½çš„å¼€å§‹/åœæ­¢æŒ‰é’®ã€‚
          const handleHomeStartStop = async () => {
            if (savingAuto.value) return;
            savingAuto.value = true;
            try {
              // å…ˆè·å–æœ€æ–°è®¾ç½®
              let hasKey = false;
              let currentAuto = false;
              try {
                const r = await fetch('/api/settings');
                const d = r && r.ok ? await r.json() : null;
                hasKey = !!(d && d.ok && d.data && d.data.hasOpenaiKey);
                currentAuto = !!(d && d.ok && d.data && d.data.autoGenerate);
                srvAuto.value = currentAuto;
              } catch (_) { }

              // å¦‚æœå‡†å¤‡å¼€å¯ä½†æ²¡æœ‰ Keyï¼Œç›´æ¥é˜»æ–­
              if (!srvAuto.value && !hasKey) {
                try {
                  ElementPlus.ElMessageBox && ElementPlus.ElMessageBox.alert
                    ? ElementPlus.ElMessageBox.alert((window.I18N && window.I18N.t ? window.I18N.t('settings.autoQueueAlert') : 'å½“å‰æœªè®¾ç½® OpenAI Keyï¼Œè‡ªåŠ¨ä»»åŠ¡å°†æ— æ³•å¼€å§‹ï¼Œè¯·å…ˆå¡«å†™ Key å¹¶ä¿å­˜'), 'Settings')
                    : ElementPlus.ElMessage.warning((window.I18N && window.I18N.t ? window.I18N.t('settings.autoQueueAlert') : 'å½“å‰æœªè®¾ç½® OpenAI Keyï¼Œè‡ªåŠ¨ä»»åŠ¡å°†æ— æ³•å¼€å§‹ï¼Œè¯·å…ˆå¡«å†™ Key å¹¶ä¿å­˜'));
                } catch (_) { ElementPlus.ElMessage.warning('OpenAI Key required'); }
                return;
              }

              // ç›®æ ‡çŠ¶æ€ï¼šåˆ‡æ¢ï¼ˆä¸æ–‡ä»¶é¡µä¸€è‡´ï¼‰
              const target = !srvAuto.value;
              const resp = await fetch('/api/settings', {
                method: 'POST',
                headers: { 'Content-Type': 'application/json' },
                body: JSON.stringify({
                  autoGenerate: target,
                  autoTranslate: false,
                })
              });
              const data = await resp.json().catch(() => null);
              if (resp.ok && data && data.ok) {
                srvAuto.value = target;
                ElementPlus.ElMessage.success((window.I18N && window.I18N.t) ? window.I18N.t('msg.ok') : 'Success');
                // æˆåŠŸåè·³è½¬åˆ°æ–‡ä»¶é¡µ
                window.location.href = '/blank';
              } else {
                ElementPlus.ElMessage.error((data && data.message) || ((window.I18N && window.I18N.t) ? window.I18N.t('msg.saveFail') : 'Save failed'));
              }
            } catch (e) {
              ElementPlus.ElMessage.error(((window.I18N && window.I18N.t) ? window.I18N.t('msg.netErr') : 'Network error') + ': ' + ((e && e.message) || e));
            } finally {
              savingAuto.value = false;
