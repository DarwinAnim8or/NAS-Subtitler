<script>
  (function () {
    const files = document.getElementById('files');
    const btn = document.getElementById('viewToggle');
    if (!files || !btn) return;
    const saved = (localStorage.getItem('blank_view') || 'grid');
    applyView(saved);
    btn.addEventListener('click', () => {
      const current = btn.dataset.view === 'grid' ? 'list' : 'grid';
      applyView(current);
      try { localStorage.setItem('blank_view', current); } catch (e) { }
    });
    function applyView(view) {
      btn.dataset.view = view;
      btn.setAttribute('aria-pressed', view === 'list');
      files.classList.toggle('grid', view === 'grid');
      files.classList.toggle('list', view === 'list');
    }
  })();
</script>
<script>
