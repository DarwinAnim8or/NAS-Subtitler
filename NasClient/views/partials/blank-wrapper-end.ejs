  (function () {
    const el = document.getElementById('langSwitchApp');
    if (!el || !window.Vue || !window.ElementPlus) return;
    const { createApp, ref, watch } = Vue;
    const saved = (localStorage.getItem('ui_lang') || 'zh-CN');
